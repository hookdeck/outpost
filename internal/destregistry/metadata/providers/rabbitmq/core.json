{
  "type": "rabbitmq",
  "config_fields": [
    {
      "key": "server_url",
      "type": "text",
      "label": "Server URL",
      "description": "The RabbitMQ server URL (e.g., myrabbitmq.com:5672)",
      "required": true,
      "pattern": "^(?:localhost|127\\.0\\.0\\.1|\\[::1\\]|(?:[\\w\\-]+\\.)+[a-z]{2,})(?::\\d{1,5})?$"
    },
    {
      "key": "exchange",
      "type": "text",
      "label": "Exchange",
      "description": "The RabbitMQ exchange to publish messages to",
      "required": true
    },
    {
      "key": "routing_key",
      "type": "text",
      "label": "Routing Key",
      "description": "The routing key for message routing",
      "required": false
    }
  ],
  "credential_fields": [
    {
      "key": "username",
      "type": "text",
      "label": "Username",
      "description": "RabbitMQ username",
      "required": true,
      "sensitive": false
    },
    {
      "key": "password",
      "type": "text",
      "label": "Password",
      "description": "RabbitMQ password",
      "required": true,
      "sensitive": true
    }
  ]
}
