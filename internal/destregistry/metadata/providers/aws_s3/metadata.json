{
  "type": "aws_s3",
  "label": "AWS S3",
  "description": "Store events in an Amazon S3 bucket",
  "link": "https://aws.amazon.com/s3/",
  "config_fields": [
    {
      "key": "bucket",
      "type": "text",
      "label": "Bucket Name",
      "description": "The name of your AWS S3 bucket",
      "required": true
    },
    {
      "key": "region",
      "type": "text",
      "label": "AWS Region",
      "description": "The AWS region where your bucket is located",
      "required": true,
      "pattern": "^[a-z]{2}-[a-z]+-[0-9]+$"
    },
    {
      "key": "key_template",
      "type": "text",
      "label": "Object Key Template",
      "description": "JMESPath expression for generating S3 object keys. Default: join('', [time.rfc3339_nano, '_', metadata.\"event-id\", '.json'])",
      "required": false,
      "placeholder": "join('/', [time.year, time.month, time.day, metadata.\"event-id\", '.json'])"
    },
    {
      "key": "storage_class",
      "type": "text",
      "label": "S3 Storage Class",
      "description": "The storage class for the S3 objects (e.g., STANDARD, INTELLIGENT_TIERING, GLACIER, etc.)",
      "required": false,
      "default": "STANDARD"
    }
  ],
  "credential_fields": [
    {
      "key": "key",
      "type": "text",
      "label": "Access Key ID",
      "description": "AWS Access Key ID",
      "required": true,
      "sensitive": true
    },
    {
      "key": "secret",
      "type": "text",
      "label": "Secret Access Key",
      "description": "AWS Secret Access Key",
      "required": true,
      "sensitive": true
    },
    {
      "key": "session",
      "type": "text",
      "label": "Session Token",
      "description": "AWS Session Token (optional)",
      "required": false,
      "sensitive": true
    }
  ],
