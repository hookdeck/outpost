{
  "type": "aws_sqs",
  "config_fields": [
    {
      "key": "queue_url",
      "type": "text",
      "label": "Queue URL",
      "description": "The URL of your AWS SQS queue",
      "required": true,
      "pattern": "^https?:\\/\\/(?:localhost|127\\.0\\.0\\.1|\\[::1\\]|(?:[\\w\\-]+\\.)+[a-z]{2,})(?::\\d{1,5})?(?:\\/[\\w\\-\\/\\.~:?#\\[\\]@!$&'\\(\\)*+,;=]*)?$"
    },
    {
      "key": "endpoint",
      "type": "text",
      "label": "Endpoint",
      "description": "Optional custom endpoint for AWS SQS",
      "required": false,
      "pattern": "^https?:\\/\\/(?:localhost|127\\.0\\.0\\.1|\\[::1\\]|(?:[\\w\\-]+\\.)+[a-z]{2,})(?::\\d{1,5})?(?:\\/[\\w\\-\\/\\.~:?#\\[\\]@!$&'\\(\\)*+,;=]*)?$"
    }
  ],
  "credential_fields": [
    {
      "key": "key",
      "type": "text",
      "label": "Access Key ID",
      "description": "AWS Access Key ID",
      "required": true,
      "sensitive": true
    },
    {
      "key": "secret",
      "type": "text",
      "label": "Secret Access Key",
      "description": "AWS Secret Access Key",
      "required": true,
      "sensitive": true
    },
    {
      "key": "session",
      "type": "text",
      "label": "Session Token",
      "description": "AWS Session Token (optional)",
      "required": false,
      "sensitive": true
    }
  ]
}
